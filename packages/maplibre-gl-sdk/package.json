{
  "name": "@mapka/maplibre-gl-sdk",
  "version": "0.14.0",
  "type": "module",
  "description": "Mapka JS SDK",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mapka-dev/sdk.git"
  },
  "exports": {
    ".": "./lib/index.js",
    "./styles.css": "./lib/styles.css"
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "styles": "lightningcss --minify --bundle ./src/styles.css -o lib/styles.css",
    "styles:watch": "lightningcss --minify --bundle ./src/styles.css -o lib/styles.css"
  },
  "keywords": [
    "map",
    "maplibre",
    "mapka",
    "sdk"
  ],
  "author": "chyzwar",
  "license": "Apache-2.0",
  "dependencies": {
    "es-toolkit": "^1.43.0",
    "html-to-image": "^1.11.13",
    "maplibre-gl": "^5.15.0",
    "preact": "^10.28.0",
    "terra-draw": "^1.21.2",
    "terra-draw-maplibre-gl-adapter": "^1.2.3"
  },
  "devDependencies": {
    "@chyzwar/tsconfig": "^0.4.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/preact": "^3.2.4",
    "happy-dom": "^20.0.11",
    "lightningcss": "^1.31.1",
    "lightningcss-cli": "^1.31.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "files": [
    "src/",
    "lib/",
    "dist",
    "!**/__tests__/",
    "!lib/buildInfo.json"
  ]
}
